msgid "internet"
msgstr "インターネット"

#: ../netpanel/moblin-panel-web.cc:216
#: ../netpanel/moblin-panel-web.cc:262
msgid "Internet"
msgstr "インターネット"

#: ../netpanel/moblin-panel-internet.desktop.in.in.h:1
msgid "Internet panel"
